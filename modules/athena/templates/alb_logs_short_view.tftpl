CREATE OR REPLACE VIEW "${view_name}" AS
SELECT
    day,
    time,
    request_processing_time,
    target_processing_time,
    elb_status_code,
    target_status_code,
    client_ip,
    request_verb AS "request_method",
    request_url,
    user_agent
FROM alb_logs_full
WHERE day > date_format(current_date - interval '${days}' day, '%Y/%m/%d');
